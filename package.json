{
  "name": "linter-pylint",
  "linter-package": true,
  "main": "./lib/main",
  "version": "1.0.2",
  "description": "Lint python on the fly, using pylint",
  "repository": "https://github.com/AtomLinter/linter-pylint",
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "engines": {
    "atom": ">=1"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^3.2.0",
    "atom-package-deps": "^2.0.2",
    "lodash": "^3.10.1"
  },
  "package-deps": [
    "linter"
  ],
  "devDependencies": {
    "coffeelint": "^1.12.1"
  }
}

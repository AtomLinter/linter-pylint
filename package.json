{
  "name": "linter-pylint",
  "linter-package": true,
  "main": "./lib/main",
  "version": "1.1.0",
  "description": "Lint python on the fly, using pylint",
  "repository": "https://github.com/AtomLinter/linter-pylint",
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/coffeelint lib"
  },
  "engines": {
    "atom": ">=1"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^3.3.9",
    "atom-package-deps": "^3.0.5",
    "lodash": "^3.10.1"
  },
  "devDependencies": {
    "coffeelint": "^1.14.1",
    "eslint": "^1.10.2",
    "babel-eslint": "^4.1.6",
    "eslint-config-airbnb": "latest"
  },
  "package-deps": [
    "linter"
  ],
  "eslintConfig": {
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "comma-dangle": 0
    }
  }
}

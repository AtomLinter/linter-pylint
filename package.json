{
  "name": "linter-pylint",
  "main": "./lib/main.coffee",
  "version": "1.2.0",
  "private": true,
  "description": "Lint python on the fly, using pylint",
  "repository": "https://github.com/AtomLinter/linter-pylint",
  "license": "MIT",
  "scripts": {
    "test": "apm test",
    "lint": "coffeelint lib & eslint spec"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^5.0.0",
    "atom-package-deps": "^4.0.1",
    "lodash": "^4.6.1"
  },
  "devDependencies": {
    "coffeelint": "^1.15.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb-base": "^2.0.0",
    "eslint-plugin-import": "^1.6.1"
  },
  "package-deps": [
    "linter"
  ],
  "eslintConfig": {
    "extends": "airbnb-base",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    },
    "rules": {
      "comma-dangle": 0
    }
  }
}
